local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Kak.Hub All In One Script", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

